{
    "name": "gielfeldt/jwt-middleware",
    "description": "Jwt middleware for lcobucci jwt",
    "keywords": [
        "jwt",
        "jku",
        "middleware"
    ],
    "homepage": "https://github.com/gielfeldt/jwt-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gielfeldt",
            "email": "thomas@gielfeldt.dk"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "psr/http-server-middleware": "^1.0",
        "lcobucci/jwt": "^4.1",
        "psr/simple-cache": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gielfeldt\\JwtMiddleware\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6",
        "nyholm/psr7": "^1.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Gielfeldt\\JwtMiddleware\\Tests\\": "tests/"
        }
    }
}
